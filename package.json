    "publish": true
  },
  "scripts": {
    "cf-typegen": "wrangler types",
    "deploy:prod": "wrangler deploy --env prod",
    "deploy:dev": "wrangler deploy --env dev",
    "dev": "wrangler dev --env dev --host 0.0.0.0",
